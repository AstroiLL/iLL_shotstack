{
  "name": "SingleTest",
  "resourcesDir": "Content",
  "template": {
    "timeline": {
      "tracks": [
        {
          "clips": [
            {
              "asset": {
                "type": "video",
                "src": "{{Content/1-Avatar.mov}}",
                "trim": 0
              },
              "start": 0.0,
              "length": 3.0,
              "fit": "cover",
              "effect": "zoomIn",
